{
  "name": "@nayms/contracts",
  "version": "1.0.0-1133",
  "description": "Nayms smart contracts",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "buildConfig.json",
    "contracts.generated.js",
    "build/**",
    "utils/**",
    "contracts/**",
    "deployedAddresses.json",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "devnet": "ganache-cli --gasLimit 8000000 -a 50 -e 1000000 -m 'funny door sample enrich female wedding stereo crane setup shop dwarf dismiss'",
    "devnet-ci": "yarn devnet >ganache.log 2>&1 &",
    "truffle": "babel-node node_modules/.bin/truffle",
    "clean": "rm -rf ./build",
    "test": "yarn truffle test --network test",
    "trace": "MODE=trace yarn test",
    "coverage": "rm -rf coverage && yarn truffle run coverage --network coverage",
    "copy-maker-otc-artifacts": "./scripts/copyMakerOtcArtifacts.js",
    "generate-index": "./scripts/generateIndex.js",
    "inc-version": "./scripts/generateVersionInfo.js",
    "extract-deployed-addresses": "./scripts/extractDeployedAddresses.js",
    "build-admin-dapp": "./scripts/generateAdminDapp.js && node_modules/.bin/solui publish-folder --spec ./dapp-generated.json --artifacts ./build/contracts --folder ./dapp",
    "compile": "rm -rf build && yarn truffle compile --network test && yarn copy-maker-otc-artifacts && yarn generate-index",
    "deploy:mainnet": "yarn truffle migrate --network mainnet && yarn extract-deployed-addresses",
    "deploy:rinkeby": "yarn truffle migrate --network rinkeby && yarn extract-deployed-addresses",
    "deploy:local": "yarn truffle migrate --network test",
    "deploy-admin-dapp": "yarn build-admin-dapp && gh-pages -t -d dapp -e $(./scripts/outputVersion.js) -u \"Nayms Engineering <engineering@nayms.io>\"",
    "setup": "[[ -f .deployment.js ]] || cp .deployment-sample.js .deployment.js",
    "lint": "solhint --config ./.solhint.json contracts/**/*.sol",
    "abi-encode": "./scripts/abiEncode.js"
  },
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "ISC",
  "dependencies": {
    "@solui/cli": "^1.9.1",
    "argv": "0.0.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.2",
    "commander": "^2.18.0",
    "coveralls": "^3.0.6",
    "delay": "^4.3.0",
    "eth-gas-reporter": "^0.1.9",
    "eth-hd-wallet": "^0.5.1",
    "ethereum-event-logs": "^1.3.1",
    "ethereum-input-data-decoder": "0.0.3",
    "etherscan-api": "^3.0.11",
    "ethval": "^1.3.0",
    "faker": "^4.1.0",
    "fs-extra": "^8.1.0",
    "ganache-cli": "^6.7.0",
    "get-stdin": "^7.0.0",
    "gh-pages": "^2.2.0",
    "git-last-commit": "^1.0.0",
    "glob": "^7.1.6",
    "got": "^11.1.4",
    "husky": "^3.0.3",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.11",
    "moment": "^2.18.1",
    "prompt": "^1.0.0",
    "sol-merger": "^0.1.2",
    "solc": ">=0.6.7",
    "solgraph": "^0.3.2",
    "solhint": "^2.2.0",
    "solidity-coverage": "^0.7.9",
    "truffle": "^5.0.40",
    "truffle-hdwallet-provider": "^1.0.0-web3one.0",
    "uuid": "^3.0.1",
    "web3": "1.2.2",
    "web3-eth-abi": "^1.0.0-beta.55",
    "yargs": "^9.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}